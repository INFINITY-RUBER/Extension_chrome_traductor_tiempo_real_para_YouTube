{
  "manifest_version": 2,
  "name": "traductor tiempo real",
  "description": "Extension para traductor en tiempo real en tu idioma",
  "version": "0.1",
  "icons": {
    "128": "128.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "check.png",
    "default_title": "YOUTOBE TRADUCTOR A VOZ"
  }
}
